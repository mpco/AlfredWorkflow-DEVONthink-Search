				<key>escaping</key>
				<integer>68</integer>
				<key>script</key>
				<string>on run argv
	set theQuery to item 1 of argv
	set theNewQuery to (do shell script "echo '" &amp; theQuery &amp; "' | perl -CIOED -p -e 's/(\\p{Block=CJK_Unified_Ideographs}+)/ ~\\1 /g'")
	tell application "DEVONthink 3" to activate
	tell application "System Events" to tell process "DEVONthink 3"
		keystroke "n" using {command down, option down} -- open new window
		set focused of text field 1 of group 3 of toolbar 1 of window 1 to true -- focus on search field
		set value of text field 1 of group 3 of toolbar 1 of window 1 to theNewQuery
		delay 0.1
		keystroke return
	end tell
end run</string>
